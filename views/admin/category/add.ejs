
<%- include ../../layouts/header %>

<!-- Default form contact -->
<form class="text-center border border-light p-5" method="POST" action="/admin/category/add">

  <p class="h4 mb-4">Add Category</p>

  <!-- Name -->
  <input type="text" name="title"id="defaultContactFormName" class="form-control mb-4" placeholder="Title"value="<% title %>">

  <!-- Email -->
  <input type="text" name="desc"id="defaultContactFormEmail" class="form-control mb-4" placeholder="Description"value="<% desc %>">



  <!-- Send button -->
  <button class="btn btn-info btn-block" type="submit">Send</button>

</form>
<!-- Default form contact -->

<%- include ../../layouts/footer %>
